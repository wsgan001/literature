@Article{alotibi2016,
  author   = {Gaseb Alotibi/Plymouth_University},
  title    = {User Profling from Network Traffc via Novel Application-Level Interactions 通过新的应用层交互进行网络流量用户分析},
  journal  = {ICITST The 11th International Conference for Internet Technology and Secured Transactions (ICITST-2016)},
  year     = {2016},
  abstract = {内部滥用已成为组织的重大问题。传统信息安全侧重于来自外部的威胁，而不是员工。
已经开展了广泛的研究来开发检测内部人员的方法 - 通常被称为数据丢失预防（DLP）工具。
不幸的是，这些工具的基本限制是它们将解决的信息提供给IP地址而不是人员。
这假设IP是静态的并且可以链接到个人，这通常不是这种情况。
由于设备的移动的本质和IP地址的动态分配，IP越来越不可靠。
本文建立在以前的工作基础上，提出和调查基于生物识别的行为模型，它是从一个新特征提取过程创建的，该过程识别从
原始网络传输元数据获取的用户的应用程序级交互（例如，不仅仅是他们正在访问Facebook，而是他们是发布，阅读还是观看视频 ）。

它也进行了描述各种类型的可以从应用程序中导出的用户交互。
该模型的验证是通过在27个参与者的2个月内收集62 GB的元数据进行的。前三名应用Skype，Hotmail和BBC中用户的平均成绩分别为98.1％，96.2％和81.8％},
  file     = {alotibi2016.pdf:ieee\\alotibi2016.pdf:PDF},
  review   = {只看了摘要
该模型的验证是通过在27个参与者的2个月内收集62 GB的元数据进行的},
}

@Article{yin2016,
  author   = {Yifang Yin/National_University_of_Singapore},
  title    = {Encoded Semantic Tree for Automatic UserProfiling Applied to Personalized Video Summarization 用于自动用户分析的 编码语义树 应用于个性化视频摘要},
  journal  = {SUBMISSION TO IEEE TRANSACTIONS ON CIRCUITS AND SYSTEMS FOR VIDEO TECHNOLOGY 视频类顶刊},
  year     = {2016},
  abstract = {我们提出一种创新的方法自动视频摘要生成与个人适应。
用户兴趣从他们的个人图像集合中挖掘出来。
为了减少语义差距，我们提出基于新颖的语义树（SeTree）提取视觉表示。SeTree是一个层次结构，可以捕获码本中的视觉场景之间的概念关系。
这个想法建立在以下观点之上：元素之间的这种语义连接在以前的工作中被忽略。
为了构建SeTree，我们采用了一个规范化的图形剪切聚类算法，通过结合使用视觉特征，文本信息和社会用户图像连接。
通过使用这种技术，与现有方法相比，我们在个性化视频段排序中获得了8.1％的归一化折扣累积增益（nDCG）的改善。

此外，为了促进视频的有趣部分，我们提取时空显着图，并通过内核拟合和匹配来估计段的吸引力。
利用线性函数来组合两个因素，基于该因素，视频的重放速率适合于生成摘要。
我们以快速模式播放不太重要的细分，以便用户更新上下文。进行了主观实验，表明我们提出的视频摘要方法胜过最先进的技术6.2％},
  file     = {yin2016.pdf:ieee\\yin2016.pdf:PDF},
  review   = {视频类},
}

