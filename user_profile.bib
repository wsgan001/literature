@Article{2016-ACM-Personalised,
  author   = {Gabriella Kazai/Lumi_News},
  title    = {Personalised News and Blog Recommendations based on User Location, Facebook and Twitter User Profiling 基于用户位置，Facebook和Twitter用户分析的个性化新闻和博客推荐},
  journal  = {SIGIR ’16, July 17-21, 2016, Pisa, Italy},
  year     = {2016},
  abstract = {此演示提供了一个原型移动应用程序通过利用和组合用户的位置，向用户提供开箱即用的个性化内容建议，他们的Facebook和/或Twitter Feed以及他们的应用内动作来自动推断他们的兴趣。
我们为每个用户和每个位置构建单个模型。在检索时，我们通过将不同的基于内容的推荐内容与内容直接从他们的Facebook / Twitter Feed，本地热门文章和通过其应用内社交网络传播的内容混合构建用户的个性化Feed。
用户与其建议的交互的显式和隐含的反馈信号都用于更新他们的兴趣模型，并通过不同的内容来源学习他们的偏好。},
  file     = {2016-ACM-Personalised News and Blog Recommendations based on User Location, Facebook and Twitter User Profiling.pdf:2016-ACM-Personalised News and Blog Recommendations based on User Location, Facebook and Twitter User Profiling.pdf:PDF},
  review   = {只看了实验部分
我们的最终目标是制作一个人们想要每天使用的应用程序：我们希望优化用户保留。
然而，衡量对 留存率 的影响需要很长时间，我们已经找到了一个与之密切相关的指标，
即用户花费超过三十秒阅读的建议数量（从现在开始我们将其称为“读”）。
这是一个用于新闻应用程序的自然指标，它允许我们在尝试不同的推荐算法时迭代更快。

为了说明用户群体的自然变化以及不同用户的阅读倾向，我们对单独的建议使用拆分测试，
以便每个用户从各种来源或算法获取建议。这使我们能够看到有多少用户喜欢将一个源或算法优先于另一个，
提供一种可靠的方式来确定建议是否正常工作。如果我们要在整个用户分割建议，
使一些用户得到了一种类型的建议，而有些用户则有另外一个，这将是很难相信我们的结果，因为有些用户阅读，自然比别人多。

We use a Bayesian formula due to Evan Miller4 to compare two types of recommendations.
数据源：http://www.evanmiller.org/bayesian-abtesting.html},
}

@Article{2016-acm-Profiling,
  author   = {Pengyuan Zhou/University of Helsinki, Finland},
  title    = {Profiling and Grouping Users to Edge Resources According to User Interest Similarity 根据用户兴趣相似性对用户进行边缘资源分析和分组},
  journal  = {CAN’16, December 12 2016, Irvine, CA, USA},
  year     = {2016},
  abstract = {云计算为大规模分布式应用程序提供了一个共享的资源池。
雾化计算和边缘计算等最新趋势使得云端的工作量更接近网络和用户的边缘。
有效利用边缘资源需要管理资源，并将用户流量引导到正确的边缘服务器。
在本文中，我们建议根据用户的兴趣情况对用户进行配置和分组。
我们认为边缘缓存是一个例子，通过我们的评估显示将用户从同一个组引导到同一个缓存的潜在好处。
我们调查一系列工作负载和参数，并得出相同的结论。
我们的研究结果突出了用户分组的重要性，并展示了这种方法的潜在优势。},
  file     = {2016-acm-Profiling and Grouping Users to Edge Resources According to User Interest Similarity.pdf:2016-acm-Profiling and Grouping Users to Edge Resources According to User Interest Similarity.pdf:PDF},
  review   = {只看了实验部分
使用伊卡洛斯作为评估分组性能的模拟器。Icarus是用于评估缓存性能的ICN模拟器[16]
对于用户兴趣分配函数，我们使用广泛接受的Zipfian分布},
}

@Article{2016-acm-UserProfilinginthe,
  author   = {Roberto.Gonzalez/NEC.Labs.Europe},
  title    = {User Profiling in the Time of HTTPS},
  journal  = {IMC 2016, November 14-16, 2016, Santa Monica, CA, USA},
  year     = {2016},
  abstract = {跟踪网站内和网站之间的用户是通过有针对性的广告和大数据分析来获取利益，人口统计类型和其他信息的基础。
HTTPS的到来应该使得对于任何超出通信端点的人来说，更难以进行分析。在本文中，我们研究了上述情况在多大程度上是真实的。

我们首先通过了解用户访问的域名，无论是通过TLS协议或通过DNS服务器名称指示，窃听者已经可以得到基本分析的信息，尤其是对于其内容是同质域。
对于依赖于用户访问的特定页面的各种类别的域，例如新闻门户，电子商务站点等，基本分析技术失败。
然而，通过使用的流量指纹识别仍然可以进行准确的分析网络流量签名来推断用户浏览的确切页面，甚至在HTTPS下。
我们证明，传输层指纹识别保持稳健可扩展性，尽管存在诸如缓存，不同设备类型的动态内容等障碍。
总体而言，我们的结果表明，尽管HTTPS使得分析更加困难，但它并没有以任何方式消除它。},
  file     = {2016-acm-UserProfilinginthe Time of HTTPS.pdf:2016-acm-UserProfilinginthe Time of HTTPS.pdf:PDF},
  review   = {仅看摘要，从http协议获取信息},
}

