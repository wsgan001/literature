@Article{yin2016,
  author   = {Yifang Yin/National_University_of_Singapore},
  title    = {Encoded Semantic Tree for Automatic UserProfiling Applied to Personalized Video Summarization 用于自动用户分析的 编码语义树 应用于个性化视频摘要},
  journal  = {SUBMISSION TO IEEE TRANSACTIONS ON CIRCUITS AND SYSTEMS FOR VIDEO TECHNOLOGY 视频类顶刊},
  year     = {2016},
  abstract = {我们提出一种创新的方法自动视频摘要生成与个人适应。
用户兴趣从他们的个人图像集合中挖掘出来。
为了减少语义差距，我们提出基于新颖的语义树（SeTree）提取视觉表示。SeTree是一个层次结构，可以捕获码本中的视觉场景之间的概念关系。
这个想法建立在以下观点之上：元素之间的这种语义连接在以前的工作中被忽略。
为了构建SeTree，我们采用了一个规范化的图形剪切聚类算法，通过结合使用视觉特征，文本信息和社会用户图像连接。
通过使用这种技术，与现有方法相比，我们在个性化视频段排序中获得了8.1％的归一化折扣累积增益（nDCG）的改善。

此外，为了促进视频的有趣部分，我们提取时空显着图，并通过内核拟合和匹配来估计段的吸引力。
利用线性函数来组合两个因素，基于该因素，视频的重放速率适合于生成摘要。
我们以快速模式播放不太重要的细分，以便用户更新上下文。进行了主观实验，表明我们提出的视频摘要方法胜过最先进的技术6.2％},
  file     = {yin2016.pdf:ieee\\yin2016.pdf:PDF},
  review   = {视频类},
}

@Article{wang2016,
  author   = {Sisi_Wang/University_of_Washington_Tacoma},
  title    = {VirtualIdentity: Privacy Preserving User Profiling 虚拟身份：隐私保护用户分析},
  journal  = {ASONAM(EI检索)},
  year     = {2016},
  abstract = {用户生成内容（UGC）的用户分析是支持许多社交媒体公司的商业模式的常见做法。
现有系统要求UGC完全暴露于构建用户配置文件的模块。
在本文中，我们展示了可以在不访问用户原始数据的情况下构建用户配置文件，
并且不会向社交媒体网站的用户暴露用于用户分析的受过训练的机器学习模型（作为公司的知识产权）。
我们提出虚拟身份，一种应用程序，使用安全的多方加密协议来检测用户的年龄，性别和个性特征，通过使用经过训练的支持向量机模型以隐私保护方式对其用户生成的文本和个人图片进行分类},
  file     = {wang2016.pdf:ieee\\wang2016.pdf:PDF},
}

@Article{sahil2016,
  author   = {Sahi/Punjab_Institute_of_Technology_Kapurthala},
  title    = {Designing and Analysis of User Profiling System forCloud Computing Security using Fuzzy guided Genetic Algorithm 使用模糊引导遗传算法进行云计算安全用户分析系统的设计与分析},
  journal  = {ICCCA2016},
  year     = {2016},
  abstract = {云计算在一方面以按付费方式提供可扩展的虚拟计算资源，以网络服务的形式; 但另一方面，也引起了对环境外部和内部对云环境的安全威胁的担忧。
现有的安全系统无法承受威胁的动态性质，因此现有的安全系统需要进行某种扩充，这将使安全系统能够以更突出的方式工作。VM之间的攻击在云环境中具有这种威胁。
用户分析在这种情况下可以通过分析用户的使用模式和从安全系统的这些使用模式提供合理的解释来帮助，然后，安全系统基于这些解释，以反应和主动的方式对分析威胁情况。
本文是基于我们最后一篇论文的发现，我们发现基于人工智能技术的用户分析系统的设计：模糊和遗传算法单独不足以提供全面的用户分析系统，但可能是可能的 以混合方式使用这两种方法来设计用于云计算安全性的全面的用户分析系统。因此，我们使用混合方法模糊导向遗传算法设计用户分析系统，发现它涵盖了我们以前的工作分析的所有研究差距，甚至完成了评估框架的所有七个原则},
  file     = {sahil2016.pdf:ieee\\sahil2016.pdf:PDF},
  review   = {无通用数据},
}

