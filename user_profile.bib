@Article{Wide&Deep,
  author        = {Heng-Tze Cheng,},
  title         = {Wide \& Deep Learning for Recommender Systems},
  journal       = {arXiv},
  year          = {2016},
  __markedentry = {[windows:1]},
  abstract      = {具有非线性特征变换的广义线性模型被广泛用于具有稀疏输入的大规模回归和分类问题。
通过广泛的跨产品特征转换来记忆特征交互是有效的和可解释的，而泛化需要更多的特征工程努力。
通过较少的特征工程，深度神经网络可以通过低维密集嵌入学习稀疏特征来更好地概括看不见的特征组合。
然而，当用户 - 项目交互是稀疏和高阶的时，带有嵌入的深层神经网络可能会过度泛化并推荐不太相关的项目。
在本文中，我们提出了广泛和深度学习|联合训练的宽线性模型和深度神经网络|来结合推荐系统的记忆和泛化的好处。
我们在Google Play上制作并评估了该系统，该商业移动应用商店拥有超过10亿活跃用户和超过100万个应用。
在线实验结果显示，Wide＆Deep显着增加了应用程序采集，与仅广泛和仅限深度的模式相比。
我们也在TensorFlow中开源实施。},
  file          = {Wide&Deep.pdf:Wide&Deep.pdf:PDF},
}

